}
.plain_style_tbl{
    background: none !important;
    vertical-align: middle;
    margin: auto;
    
}
.plain_style_tbl th{
    
   
    background: none !important;
}
.icon{
    width:21px;
    height:19px;
    margin: 2px;
    cursor: pointer;
}
.icon:hover{
    width:25px;
    height:23px;
    margin: 0px;
    cursor: pointer;
}
.link_num{
    display: inline;
    position:relative;
    width:20px;
    border: 1px solid rgb(145, 223, 30);
    margin: 1px;
    padding-left: 4px;
    padding-right: 4px;
}
.link_num:hover{
    background: whitesmoke;
    border: 1px solid gray;
}