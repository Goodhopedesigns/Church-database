/* default css */
 body{
  background-color:white;
  font-family: Arial, Verdana, sans-serif;
  margin:auto;
  padding:0px;
  }
             
#wrapper{
    width:980px;
    /*margin-top:30px;*/ 
    margin: 2px auto;
    padding-top:10px;
    border-radius:15;
/*    background: #252324;*/
    /* Safari 5.1+, Chrome 10+ */
    background: -webkit-radial-gradient(circle,rgb(247, 236, 236),rgba(46, 230, 46, 0.9),rgba(191, 245, 122, 0.56) ); 
    /* Firefox 3.6+ */
    background: -moz-radial-gradient(circle,rgba(250, 238, 237, 0.4),rgba(71, 211, 71, 0.9),rgba(255, 255, 255, 0.84) );
/*    background-color:rgba(235, 227, 43, 0.4);*/
    /*  border:2px solid #efdda8; */
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#7FFFD4', endColorstr='#ADFF2F', GradientType=0);
    box-shadow: 0px 0px 2px grey;


}
table{
   
}
tr{
/*   border-bottom: 1px solid khaki;*/
/*   border-top: 1px dotted black;*/
   height: 22px; 
}
td,th{
   text-align:left;
   vertical-align:central;
   padding-left: 5px;
   padding-righ: 5px;
}

th{
   background: rgb(255, 217, 202);
   font-size: 11px;
}
#profile{
  margin:10px auto;
  font-weight:bold;
  width: 480px;
  float: left;
} 
  h1 { 
  font-size:1.2em; 
  color:#14b8b8; 
 
} 
 
h2{ 
  font-size:1em; 
  color:#14b8b8; 
} 
 
h3{ 
  font-size:.9em; 
  color:#14b8b8; 
} 
  
  h4{ 
  font-size:.8em; 
  color:#14b8b8; 
} 
  	 
  #nav{
    float:left;
    width:135px;
    height:auto;
  }
  #online_support{
    position: relative;
    float:right;  
    top: 25px;
    left:-7px;
/*    margin: 0p 10px;*/
    width: 180px;
    border: 1px groove rgba(255, 255, 255, 0.82);
    height:350px;   
  
    border-top-left-radius: 18px;
    border-bottom-right-radius: 30px;
	
    background: rgb(190, 255, 147);
} 
  #content{
    width:980px;  
  }
  #main{
    float:left;  
    margin: 25px 10px;
    width: 770px;
    border: 1px groove rgba(255, 255, 255, 0.82);
    min-height:350px;
    max-height: 550px;
    border-top-left-radius: 18px;
    border-bottom-right-radius: 30px;
    background: rgba(178, 248, 151, 0.72);
}
  #main_title, #support_title{
    margin-bottom:10px;  
    color:white;  
    position:relative;  
    background-color: rgba(9, 7, 7, 0.59);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#333333', endColorstr='#333333', GradientType=0);
    height:28px;
    padding-left: 20px;
    padding-top: 6px;
    font-size: 20px;
    border-top-left-radius: 17px;
} 
#main_title{
    text-transform: uppercase;
}
 #header{
    font-family: a;
    font-size:12px;
    font-weight: bold;
    margin-bottom:-15px;
  }
  #banner{
    height: 69px;
      
  
    text-align: center;
}
  #header h1,#header_title{
/*    margin-left: 10px;*/
    font-size: 2.3em;
    margin-top: 0px;
    text-align: center;
    text-transform: uppercase;
  
    color: palegreen;
    text-shadow: 0px 2px 7px black;
}
#welcome_home{
    height: 118px;
    margin:10px auto;
    width:620px;
    border:1px solid white;
    border-radius: 10px;
    padding: 12px;
    background-color: rgba(255, 255, 255, 0.26);
}
#login_form{
    margin:10px auto;
    width:580px;
    
}
  #search_div{
  float:right;
  }           
  #footer{
    color:black;
    clear:both;
    padding-top:10px;
    font-size:1em;
	height:25px;
	border-top:2px groove #92CAC1;
	border-bottom:2px groove #92CAC1;
    background: rgb(250, 219, 180); 
    padding: 0;
    padding-left: 9px;
    margin-bottom:0px;
    margin-top: 20px;	
  } 
  
  p,li,td{ 
  font-size:.8em; 
}
