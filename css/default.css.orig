}
#search_box_{
    width: 558px;
    
    height: auto;
    padding: 5px 5px 1px 5px;
    background: white;
    margin-bottom: 5px;
    border-radius: 10px;
    box-shadow: inset 0px 0px 5px rgb(8, 9, 8);
}
.search_bar{
    width: 480px;
    border: none;
    height: 13px;
    padding-left: 10px;
}
.display_action{
    width:95%;
    float:right;
